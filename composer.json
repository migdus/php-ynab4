  "keywords": ["api", "ynab4", "transactions", "budget"],
  "license": "MIT",
  "type" : "library",
  "homepage": "https://github.com/petrica/php-ynab4",
  "authors": [
    {
      "name": "Petrica Martinescu",
      "email": "petrica@martinescu.com"
    }
  ],
  "support": {
    "issues": "https://github.com/petrica/php-ynab4/issues"
